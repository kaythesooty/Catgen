{
  "compilerOptions": {
    "types": ["vite/client"],
    "target": "es2023", 
    "jsx": "react-jsx",
    "module": "ES2020",
    "moduleResolution": "Bundler", 
    "noEmit": true,
    "isolatedModules": true, 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@dicts/*": ["client/dicts/*"],
      "@models/*": ["models/*"],
      "@pickers/*": ["client/components/pickers/*"],
    }
  }
}